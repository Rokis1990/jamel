<?xml version = "1.0" encoding = "UTF-8"?>

<matrix
	shortTitle = "Balance sheet"
	title="Balance sheet matrix"
	date="25-06-2015"
	content="Configuration of the macroeconomic balance sheet matrix.">

	<sector key="Households" />
	<sector key="Industry" />
	<sector key="Banks" />

	<row key="In Process">
		<Industry val="Industry_t.sum.inventories.inProcess.val" />
	</row>

	<row key="Finished Goods">
		<Industry val="Industry_t.sum.inventories.fg.val" />
	</row>

	<row key="Deposits">
		<Households val="Households_t.sum.cash" />
		<Industry val="Industry_t.sum.cash" />
		<Banks val="-Banks_t.liabilities" />
	</row>

	<row key="Loans">
		<Industry val="-Industry_t.sum.liabilities" />
		<Banks val="Banks_t.assets" />
	</row>

	<row key="Capital">
		<Households val="Households_t.sum.capital" />
		<Industry val="-Industry_t.sum.capital" />
		<Banks val="-Banks_t.capital" />
	</row>

</matrix>