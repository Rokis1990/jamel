<?xml version = "1.0" encoding = "UTF-8"?>

<validation

	title="Consistency"
	date="21-06-2015"
	content="Baseline validation tests"
	start="1">

	<test
		val1="Households_t.sum.employed"
		val2="Industry_t.sum.workforce" />
	<test
		val1="Households_t.sum.agents"
		val2="Households_t.sum.employed + Households_t.sum.unemployed" />
	<test
		val1="Households_t.sum.wages"
		val2="Industry_t.sum.wageBill" />

	<test
		val1="Households_t.sum.capital"
		val2="Industry_t.sum.capital+Banks_t.capital" />
	<test
		val1="Households_t.sum.dividend"
		val2="Industry_t.sum.dividends+Banks_t.dividends" />

	<test
		val1="Households_t.sum.capital - Households_t-1.sum.capital"
		val2="Industry_t.sum.capital - Industry_t-1.sum.capital + Banks_t.capital - Banks_t-1.capital" />
	<test
		val1="Industry_t.sum.grossProfit"
		val2="Households_t.sum.dividend + Households_t.sum.capital - Households_t-1.sum.capital" />
	<test
		val1="Industry_t.sum.grossProfit"
		val2="Industry_t.sum.dividends + Industry_t.sum.interest + Industry_t.sum.capital - Industry_t-1.sum.capital + Industry_t.sum.canceledDeposits - Industry_t.sum.canceledDebts" />
	<test
		val1="Industry_t.sum.capital - Industry_t-1.sum.capital"
		val2="Industry_t.sum.grossProfit - Industry_t.sum.dividends - Industry_t.sum.interest - Industry_t.sum.canceledDeposits + Industry_t.sum.canceledDebts" />

	<test
		val1="Industry_t.sum.sales.vol"
		val2="Households_t.sum.consumption.vol" />
	<test
		val1="Industry_t.sum.sales.val"
		val2="Households_t.sum.consumption.val" />
	<test
		val1="Industry_t.sum.inventories.fg.val + Industry_t.sum.inventories.inProcess.val"
		val2="Households_t.sum.cash + Households_t.sum.capital" />
	<test
		val1="Industry_t.sum.interest"
		val2="Banks_t.interest" />

	<test
		val1="Industry_t.sum.canceledDeposits"
		val2="Banks_t.canceledDeposits" />
	<test
		val1="Industry_t.sum.canceledDebts"
		val2="Banks_t.canceledDebts" />
	<test
		val1="Industry_t.sum.bankruptcies"
		val2="Banks_t.bankruptcies" />
	<test
		val1="Industry_t.sum.insolvents"
		val2="0" />

</validation>