<?xml version = "1.0" encoding = "UTF-8"?>

<charts
	content="Baseline configuration of the chart panels."
	date="20-06-2015">

	<panel
		title="Main"
		rows="3"
		cols="3">

		<chart
			title="Age and Productivity"
			options="scatter">
			<series
				sector="Industry_t"
				x="age"
				y="productivity"
				colors="transparentBlue"
				legend="Firms" />
		</chart>

		<chart title="Prices">
			<series
				value="Industry_t.mean.prices"
				color="red"
				label="Mean" />
			<series
				value="Industry_t.max.prices"
				color="veryLightRed"
				label="Max" />
			<series
				value="Industry_t.min.prices"
				color="veryLightRed"
				label="Min" />
		</chart>

		<chart title="Goods Market">
			<series
				value="Industry_t.sum.production.vol"
				color="red"
				label="Production" />
			<series
				value="Households_t.sum.consumption.vol"
				color="blue"
				label="Consumption" />
		</chart>

		<chart title="Inventories">
			<series
				value="Industry_t.sum.inventories.fg.vol"
				color="green"
				label="Volume" />
			<series
				value="Industry_t.sum.inventories.fg.vol.normal"
				color="orange"
				label="Target" />
		</chart>

		<chart title="Wages">
			<series
				value="Industry_t.mean.wages"
				color="blue"
				label="Mean" />
			<series
				value="Industry_t.max.wages"
				color="veryLightBlue"
				label="Max" />
			<series
				value="Industry_t.min.wages"
				color="veryLightBlue"
				label="Min" />
		</chart>

		<chart title="Unemployment and Inflation">
			<series
				value="Households_t.sum.unemployed/Households_t.sum.agents"
				color="blue"
				label="Unemployment" />
			<series
				value="(Industry_t.mean.prices-Industry_t-12.mean.prices)/Industry_t.mean.prices"
				color="red"
				label="Inflation" />
			<series
				value="((Industry_t.sum.sales.val/Industry_t.sum.sales.vol)-(Industry_t-12.sum.sales.val/Industry_t-12.sum.sales.vol))/(Industry_t-12.sum.sales.val/Industry_t-12.sum.sales.vol)"
				color="green"
				label="Inflation2" />
			<yAxis
				min="0"
				max="1" />
		</chart>

		<chart title="Bank Ratios">
			<series
				value="Banks_t.capital/Banks_t.liabilities"
				color="green"
				label="Capital" />
			<series
				value="Banks_t.doubtfulDebt/Banks_t.liabilities"
				color="red"
				label="Doubtful Debt" />
		</chart>

		<!-- <chart title="Test Income"> <series value="Industry_t.sum.grossProfit" 
			/> <series value="Industry_t.sum.dividends+Banks_t.dividends" /> </chart> -->

		<chart title="Income Distribution">
			<series
				value="Households_t.sum.wages/(Households_t.sum.wages+Households_t.sum.dividend)"
				color="blue"
				label="Wages" />
			<series
				value="Households_t.sum.dividend/(Households_t.sum.wages+Households_t.sum.dividend)"
				color="red"
				label="Dividends" />
			<yAxis
				min="0"
				max="1" />
		</chart>
	</panel>

	<panel
		title="Industry"
		rows="3"
		cols="3">

		<chart title="Workforce">
			<series
				value="Industry_t.sum.workforce"
				color="blue"
				label="Workforce" />
			<series
				value="Industry_t.sum.capacity"
				color="orange"
				label="Capacity" />
		</chart>

		<chart title="Capacity Utilization">
			<series
				value="Industry_t.sum.workforce/Industry_t.sum.capacity"
				color="blue"
				label="Capacity utilization" />
		</chart>

		<chart title="Markup">
			<series
				value="Industry_t.sum.grossProfit/Industry_t.sum.sales.costValue"
				color="blue"
				label="Mean" />
			<yAxis min="0" />
		</chart>

		<chart title="Gross Margin Ratio">
			<series
				value="Industry_t.sum.grossProfit/Industry_t.sum.sales.val"
				color="blue"
				label="Mean" />
		</chart>

		<chart title="Wages">
			<series
				value="Industry_t.mean.wages"
				color="blue"
				label="Mean" />
			<series
				value="Industry_t.max.wages"
				color="veryLightBlue"
				label="Max" />
			<series
				value="Industry_t.min.wages"
				color="veryLightBlue"
				label="Min" />
		</chart>

		<chart title="Inventories">
			<series
				value="Industry_t.sum.inventories.fg.vol"
				color="green"
				label="Volume" />
			<series
				value="Industry_t.sum.inventories.fg.vol.normal"
				color="orange"
				label="Target" />
		</chart>

		<chart title="Production">
			<series
				value="Industry_t.sum.production.vol"
				color="red"
				label="Production" />
			<series
				value="Households_t.sum.consumption.vol"
				color="blue"
				label="Consumption" />
		</chart>

		<chart title="Prices">
			<series
				value="Industry_t.sum.supply.val/Industry_t.sum.supply.vol"
				label="Average Price (Supply)" />
			<series
				value="Industry_t.sum.sales.val/Industry_t.sum.sales.vol"
				label="Average Price (Sales)" />
			<series
				value="Industry_t.sum.inventories.fg.val/Industry_t.sum.inventories.fg.vol"
				label="Average Cost (Inventories)" />
		</chart>

		<chart title="Balance sheet">
			<series
				value="Industry_t.sum.assets"
				color="blue"
				label="Assets" />
			<series
				value="Industry_t.sum.liabilities"
				color="red"
				label="Liabilities" />
			<series
				value="Industry_t.sum.capital"
				color="green"
				label="Capital" />
		</chart>

		<chart title="Financial Ratio">
			<series
				value="Industry_t.sum.liabilities/Industry_t.sum.assets"
				color="blue"
				label="Liabilities" />
			<series
				value="Industry_t.sum.liabilities.target/Industry_t.sum.assets"
				color="orange"
				label="Target" />
		</chart>

		<chart title="Other Ratios">
			<series
				value="Industry_t.sum.cash/Industry_t.sum.wageBill"
				label="Cash to Wages Ratio" />
			<series
				value="Industry_t.sum.inventories.total.val/Industry_t.sum.wageBill"
				label="Circulating Capital to Wages Ratio" />
			<series
				value="Industry_t.sum.production.val/Industry_t.sum.wageBill"
				label="Production Value to Wages Ratio" />
		</chart>

	</panel>

	<panel
		title="Firm1"
		rows="3"
		cols="3">

		<chart
			title="Wages and Prices"
			options="scatter">
			<series
				sector="Industry_t"
				x="wages"
				y="prices"
				color="transparentRed"
				select="name=Firm1"
				label="Firm1" />
			<series
				sector="Industry_t"
				x="wages"
				y="prices"
				color="transparentBlue"
				label="Other firms" />
		</chart>

		<chart title="Price">
			<series
				value="Industry_t.agent.Firm1.prices"
				color="red"
				label="Firm1" />
			<series
				value="Industry_t.mean.prices"
				color="blue"
				label="Mean" />
			<series
				value="Industry_t.max.prices"
				color="veryLightBlue"
				label="Max" />
			<series
				value="Industry_t.min.prices"
				color="veryLightBlue"
				label="Min" />
		</chart>

		<chart title="Wage">
			<series
				value="Industry_t.agent.Firm1.wages"
				color="red"
				label="Firm1" />
			<series
				value="Industry_t.mean.wages"
				color="blue"
				label="Mean" />
			<series
				value="Industry_t.max.wages"
				color="veryLightBlue"
				label="Max" />
			<series
				value="Industry_t.min.wages"
				color="veryLightBlue"
				label="Min" />
		</chart>

		<chart title="Inventories">
			<series
				value="Industry_t.agent.Firm1.inventories.fg.vol"
				color="red"
				label="Firm1" />
			<series
				value="Industry_t.agent.Firm1.inventories.fg.vol.normal"
				color="orange"
				label="Target" />
			<series
				value="Industry_t.mean.inventories.fg.vol"
				color="blue"
				label="Mean" />
		</chart>

		<chart title="Capacity Utilization">
			<series
				value="Industry_t.agent.Firm1.workforce/Industry_t.agent.Firm1.capacity"
				color="red"
				label="Firm1" />
			<series
				value="Industry_t.sum.workforce/Industry_t.sum.capacity"
				color="blue"
				label="Average" />
			<yAxis
				min="0"
				max="1" />
		</chart>

	</panel>

	<panel
		title="Bank"
		rows="3"
		cols="3">

		<chart title="Ratios">
			<series
				value="Banks_t.capital/Banks_t.liabilities"
				color="green"
				label="Capital" />
			<series
				value="Banks_t.doubtfulDebt/Banks_t.liabilities"
				color="red"
				label="Doubtful Debt" />
		</chart>

		<chart title="Dividends">
			<series
				value="Banks_t.dividends"
				color="blue"
				label="Dividends" />
		</chart>

		<chart title="Balance sheet">
			<series
				value="Banks_t.assets"
				color="blue"
				label="Assets" />
			<series
				value="Banks_t.liabilities"
				color="red"
				label="Liabilities" />
			<series
				value="Banks_t.capital"
				color="green"
				label="Capital" />
		</chart>

		<chart title="Deposits">
			<series
				value="Industry_t.sum.cash"
				label="Firms" />
			<series
				value="Households_t.sum.cash"
				label="Households" />
		</chart>

		<chart title="Velocity of Money">
			<series
				value="12*(Industry_t.sum.wageBill+Industry_t.sum.grossProfit)/Banks_t.liabilities"
				label="Velocity of Money" />
			<yAxis
				min="0"
				max="6" />
		</chart>

		<chart title="Bankruptcies">
			<series
				value="Banks_t.bankruptcies"
				label="Bankruptcies" />
		</chart>

	</panel>

	<panel
		title="Households"
		rows="3"
		cols="3">

		<chart title="Unemployment">
			<series
				value="Households_t.sum.unemployed/Households_t.sum.agents"
				label="Unemployment" />
			<yAxis
				min="0"
				max="1" />
		</chart>

		<chart title="Wealth">
			<series
				value="Households_t.sum.cash"
				label="Households Cash" />
			<series
				value="Households_t.sum.capital"
				label="Households Capital" />
		</chart>

		<chart title="Wages">
			<series
				value="Households_t.sum.wages/Households_t.sum.employed"
				label="Average" />
		</chart>

		<chart title="Consumption (Value)">
			<series
				value="Households_t.sum.consumption.budget"
				label="Consumption Budget" />
			<series
				value="Households_t.sum.consumption.val"
				label="Consumption Value" />
		</chart>

		<chart title="Consumption (Volume)">
			<series
				value="Households_t.sum.consumption.vol"
				label="Consumption (Volume)" />
		</chart>

		<chart title="Income">
			<series
				value="Households_t.sum.wages"
				label="Wages" />
			<series
				value="Industry_t.sum.grossProfit"
				label="Profits" />
		</chart>

		<chart title="Savings">
			<series
				value="(Households_t.sum.cash+Households_t.sum.capital)/(Households_t.sum.wages+Industry_t.sum.grossProfit)"
				label="Savings" />
		</chart>

		<chart title="Income Distribution">
			<series
				value="Households_t.sum.wages/(Households_t.sum.wages+Households_t.sum.dividend)"
				color="blue"
				label="Wages" />
			<series
				value="Households_t.sum.dividend/(Households_t.sum.wages+Households_t.sum.dividend)"
				color="red"
				label="Dividends" />
			<yAxis
				min="0"
				max="1" />
		</chart>

		<chart title="Dividends">
			<series
				value="Banks_t.dividends"
				label="Bank dividends" />
			<series
				value="Industry_t.sum.dividends"
				label="Industry dividends" />
		</chart>

	</panel>

</charts>