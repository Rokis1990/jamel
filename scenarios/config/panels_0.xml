<?xml version="1.0" encoding="UTF-8"?>

<!-- 14 mai 2017 -->

<gui className="jamel.gui.BasicGui">
	<panel title="Panel 1">
		<html title="Presentation">
			<center>
				<h2>A Prototype Model of a</h2>
				<h1>Monetized Economy of Production</h1>
				<h2>(May 14, 2017)</h2>
				<p>For debugging purposes.</p>
				<p>
					<a href="http://p.seppecher.free.fr/jamel/">http://p.seppecher.free.fr/jamel/</a>
				</p>
			</center>
		</html>

		<chart title="Unit Cost (red) and Unit Price (blue) (Values)">
			<series name="Unit Cost">
				<x>period</x>
				<y>
					value(Firms,supplyCost,t,sum) / value(Firms,supplyVolume,t,sum)
				</y>
				<color>red</color>
			</series>
			<series name="Unit Price">
				<x>period</x>
				<y>
					value(Firms,supplyValue,t,sum) / value(Firms,supplyVolume,t,sum)
				</y>
				<color>blue</color>
			</series>
			<xAxis label="Time">
			</xAxis>
			<yAxis>
				<min>10</min>
				<min>100</min>
			</yAxis>

		</chart>

		<chart title="Deposits of Firms (red), Workers (blue), Shareholders (green)">
			<series>
				<x>period</x>
				<y>value(Firms, money, t, sum)</y>
				<color>red</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Workers, money, t, sum)</y>
				<color>blue</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Shareholders, money, t, sum)</y>
				<color>green</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>
		<col />
		<chart title="Firms Balance Sheet">
			<series>
				<x>period</x>
				<y>value(Firms,tangibleAssets,t,sum)</y>
				<color>red</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Firms,money,t,sum)</y>
				<color>blue</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Firms,liabilities,t,sum)</y>
				<color>green</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>

		<chart title="Firms Debt Ratio">
			<series>
				<x>period/12</x>
				<y>value(Firms,liabilities,t,sum) / value(Firms,assets,t,sum)</y>
				<color>red</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>



		<chart title="Supply (red) and Demand (blue) (Values)">
			<series>
				<x>period</x>
				<y>value(Firms, supplyValue, t, sum)</y>
				<color>red</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Workers,consumptionBudget,t,sum) +
					value(Shareholders,consumptionBudget,t,sum)
				</y>
				<!-- WARNING: Il manque les shareholders, et on suppose que tous les stocks sont offerts -->
				<color>blue</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>

		<col />

		<chart title="Banks Balance Sheet">

			<series>
				<x>period</x>
				<y>value(Banks, assets, t, sum)</y>
				<color>blue</color>
			</series>

			<series>
				<x>period</x>
				<y>value(Banks, liabilities, t, sum)</y>
				<color>red</color>
			</series>

			<xAxis label="Time"></xAxis>

		</chart>

		<chart title="Production (red), Sales (blue), Consumption (green) - volumes">
			<series>
				<x>period</x>
				<y>value(Firms, productionVolume, t, sum)</y>
				<color>red</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Firms, salesVolume, t, sum)</y>
				<color>blue</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Workers,consumptionVolume,t,sum) +
					value(Shareholders,consumptionVolume,t,sum)
				</y>
				<color>green</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>

		<chart title="Supply (red) and Demand (blue) (Volumes)">
			<series>
				<x>period</x>
				<y>value(Firms, supplyVolume, t, sum)</y>
				<color>red</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Workers, consumptionBudget, t, sum) * value(Firms,
					supplyVolume, t, sum) / value(Firms, supplyValue, t, sum)
				</y>
				<!-- WARNING: Il manque les shareholders, et on suppose que tous les stocks sont offerts -->
				<color>blue</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>

	</panel>

	<panel title="Labor Market">
		<chart title="Workers (green), Employed (blue)">
			<series>
				<x>period</x>
				<y>value(Workers, countAgent, t, sum)</y>
				<color>green</color>
			</series>
			<series>
				<x>period</x>
				<y>value(Workers, employed, t, sum)</y>
				<color>blue</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>
		<chart title="Job offers">
			<series>
				<x>period</x>
				<y>value(Firms, jobOffers, t, sum)</y>
				<color>red</color>
			</series>
			<xAxis label="Time"></xAxis>
		</chart>
		<empty />
		<col />
		<empty />
		<col />
		<html title="Test Dynamic Text">
			Dynamic Text
			<br />
			Current period:
			<data>period</data>
		</html>
		<empty />
		<empty />

	</panel>

	<panel title="Panel 3">
		<empty />
	</panel>

	<panel title="Panel 4">
		<empty />
	</panel>

</gui>