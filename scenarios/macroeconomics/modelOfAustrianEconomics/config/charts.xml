				value="val(Firms, firmNumber, t)"
				color="blue"
				label="#firms" />
				
			<series
				value="val(Firms, newFirms, t)"
				color="green"
				label="#new firms" />
			<yAxis label="Banks" />
		</chart>
		
		<chart title="Bank number" type="time chart">
			<series
				value="val(Banks, bankNumber, t)"
				color="blue"
				label="#banks" />
				
			<series
				value="val(Banks, newBanks, t)"
				color="green"
				label="#new banks" />
				
			<series
				value="sum(Banks, illiquidBanks, t)"
				color="red"
				label="#illiquid banks" />
			<yAxis label="Firms" />
		</chart> 
	
		
		<chart></chart>
	</col>
			
	</panel>
	

