	<panel
		title="National Acounting">
		
	<col>
		
		<chart title="Net national income" type="time chart">
			<series
				value="sum(Households, consumptionExpenditure, t)
					+sum(Firms, machineInvestment, t) + sum(Firms, operationsInvestment, t)"
				color="blue"
				label="Gross Value of Output" />
			<series
				value="val(Firms, nominalGDP, t)"
				color="green"
				label="GDP" />
			<series
				value="val(Firms, nominalGDP, t) - sum(Firms, depreciation, t)"
				color="black"
				label="Net national income" />
			<yAxis label="Monetary units" />
		</chart>
		
			
		<chart title="GDP indices" type="time chart">
			<series
				value="val(Markets, realGDP, t)"
				color="veryLightRed"
				label="Real GDP (Index)" />
			<series
				value="val(Firms, nominalGDP_index, t)"
				color="green"
				label="Nominal GDP (Index)" />
			<yAxis label="Index (t=1 = 100)" />
		</chart>
		
	
	
		<chart title="Transaction volume" type="time chart">
			<series
				value="sum(Banks, transactionVolume, t)"
				color="transparentRed"
				label="Transaction volume" />
			<yAxis label="Monetary units" />
		</chart>
	
	
	</col>
	<col>
		
		<chart title="Expenditure compilation" type="time chart">
			<series
				value="sum(Households, consumptionExpenditure, t)"
				color="blue"
				label="Consumption expenditure" />
			<series
				value="val(Firms, nominalGDP, t) - sum(Firms, depreciation, t)"
				color="black"
				label="Net national income" />
			<yAxis label="Monetary units" />
		</chart>
		
		<chart title="Investment" type="time chart">
			<series
				value="val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t)"
				color="veryLightBlue"
				label="Gross investment" />
			<series
				value="sum(Firms, depreciation, t)"
				color="red"
				label="Depreciation" />
			<series
				value="val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t) - sum(Firms, depreciation, t)"
				color="orange"
				label="Net investment" />
			<yAxis label="Monetary units" />
		</chart>
	
		
		<chart title="Money holdings" type="time chart">
			<series
				value="sum(Households, moneyHoldings, t)"
				color="red"
				label="Households" />
			<series
				value="sum(Firms, moneyHoldings, t)"
				color="blue"
				label="Firms" />
			<series
				value="sum(Banks, moneyHoldings, t)"
				color="green"
				label="Banks" />
			<series
				value="sum(Banks, moneyHoldings, t) + sum(Firms, moneyHoldings, t)
				+sum(Households, moneyHoldings, t) + val(InvestmentBank, moneyHoldings, t)"
				color="gray"
				label="Total" />
			<yAxis label="Monetary units" />
		</chart>
	
	</col>
	<col>
	
		
		<chart title="Income compilation" type="time chart">
			<series
				value="agentValue(Markets, turnover, t, labor)"
				color="red"
				label="Compensation of employees" />
			<series
				value="val(Firms, nominalGDP, t) - sum(Firms, depreciation, t)"
				color="black"
				label="Net national income" />
			<yAxis label="Monetary units" />
		</chart>
	
	
		<chart title="Savings rate" type="time chart">
			<series
				value="(val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t)) : val(Firms, nominalGDP, t)"
				color="veryLightBlue"
				label="Gross savings rate" />
			<series
				value="(val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t) - sum(Firms, depreciation, t)) 
						: (val(Firms, nominalGDP, t) - sum(Firms, depreciation, t))"
				color="orange"
				label="Net savings rate" />
			<series
				value="(agentValue(Markets, turnover, t, equity)+agentValue(Markets, turnover, t, savings)) : sum(Households, totalBudget, t)"
				color="green"
				label="Household savings rate" />
			<yAxis label="Percent" />
		</chart>
		
		<chart></chart>

	</col>
	</panel> 
	
	
