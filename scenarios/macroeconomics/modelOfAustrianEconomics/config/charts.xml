	<panel
		title="Profit and Loss">
	
	<col>
		<chart title="Firms' profit and loss" type="time chart">
			<series
				value="sum(Firms, grossProfit, t)"
				color="lightGray"
				label="Gross profits" />
			<series
				value="sum(Firms, netProfit, t)"
				color="veryLightBlue"
				label="Net profits" />	
			<series
				value="sum(Firms, losses, t)"
				color="veryLightRed"
				label="Net losses" />
			<series
				value="0"
				color="black"/>
			<yAxis label="Monetary units" />
		</chart> 
		
		<chart title="Banks' profit and loss" type="time chart">
			<series
				value="sum(Banks, profits, t)"
				color="veryLightBlue"
				label="Profits" />
			<series
				value="sum(Banks, losses, t)"
				color="veryLightRed"
				label="Losses" />
			<series
				value="0"
				color="black"/>	
			<yAxis label="Monetary units" />
		</chart>
		
		
		<chart title="Firms: Net profit / Revenue" type="time chart">
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=1)):sum(Firms, revenue, t, and(type=1, stage=1))"
				color="blue"
				label="B1" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=2)):sum(Firms, revenue, t, and(type=1, stage=2))"
				color="veryLightBlue"
				label="B2" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=3)):sum(Firms, revenue, t, and(type=1, stage=3))"
				color="cyan"
				label="B3" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=4)):sum(Firms, revenue, t, and(type=1, stage=4))"
				color="gray"
				label="B4" />
			<yAxis label="Percent" />
		</chart>  
	</col>
	<col>
	
		<chart title="Firm bankruptcies" type="time chart">
			<series
				value="val(Firms, firmFailures, t)"
				color="gray"
				label="#bankrupt firms" />
			<yAxis label="Firms" />
		</chart> 
		
	  	<chart title="Bank bankruptcies" type="time chart">
			<series
				value="val(Banks, bankFailures, t)"
				color="gray"
				label="#bankrupt banks" />
			<yAxis label="Banks" />
		</chart> 
		
		<chart></chart>

	</col>
	<col>
	
		
		<chart title="Firm number" type="time chart">
