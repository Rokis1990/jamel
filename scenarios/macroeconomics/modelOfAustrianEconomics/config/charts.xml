<?xml version = "1.0" encoding = "UTF-8"?>

<charts>

<panel title="Key figures">
	
	<col>
	
		<chart title="GDP indices" type="time chart">
			<series
				value="val(Firms, nominalGDP_index, t)"
				color="green"
				label="Nominal GDP (Index)" />
			<series
				value="val(Markets, realGDP, t)"
				color="veryLightRed"
				label="Real GDP (Index)" />
			
			<yAxis label="Index (t=1 = 100)" />
			
		</chart>
		
		<chart title="Money holdings" type="time chart">
			<series
				value="sum(Households, moneyHoldings, t)"
				color="red"
				label="Households" />
			<series
				value="sum(Firms, moneyHoldings, t)"
				color="blue"
				label="Firms" />
			<series
				value="sum(Banks, moneyHoldings, t)"
				color="green"
				label="Banks" />
			<series
				value="sum(Banks, moneyHoldings, t) + sum(Firms, moneyHoldings, t)
				+sum(Households, moneyHoldings, t) + val(InvestmentBank, moneyHoldings, t)"
				color="gray"
				label="Total" />
			<yAxis label="Monetary units" />
			
		</chart>
		
		<chart title="Employment" type="time chart">
			<series
				value="val(Households, households, t)"
				color="blue"
				label="Households" />
			<series
				value="sum(Households, employed, t)"
				color="green"
				label="Employed" />
			<yAxis label="Households" />
			
		</chart>
		
	</col>
		
	<col>
	
		<chart title="Real consumption" type="time chart">
			<series
				value="val(Markets, realConsumption, t)"
				color="veryLightBlue"
				label="Real consumption (Index)" />
			<yAxis label="Index (t=1 = 100)" />
			
		</chart>
  		
  	
		
		<chart title="Total Production and Sales" type="time chart">
			<series
				value="sum(Firms, offeringTarget, t)"
				color="orange"
				label="Off.target" />
			<series
				value="sum(Firms, productionTarget, t)"
				color="transparentBlue"
				label="Prod.target" />
			<series
				value="sum(Firms, productionVolume, t)"
				color="red"
				label="Production" />
			<series
				value="sum(Firms, offeredGoods, t)"
				color="green"
				label="Offers" />
			<series
				value="sum(Firms, salesVolume, t)"
				color="yellow"
				label="Sales" />
			<series
				value="sum(Firms, inventoriesVolume, t)"
				color="gray"
				label="Inventories" />
			<yAxis label="Goods" />
			
		</chart> 
		
		
		<chart title="Interest rates (asked)" type="time chart">
			<series
				value="agentValue(Markets, MEAN_PRICE, t, savings)"
				color="cyan"
				label="Rate on savings" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, loans)"
				color="magenta"
				label="Rate on loans" />
			<yAxis label="Percent" />
			
		</chart>

	</col>
	
	<col>
	
		<chart title="Net national income" type="time chart">
			<series
				value="sum(Households, consumptionExpenditure, t)
					+sum(Firms, machineInvestment, t) + sum(Firms, operationsInvestment, t)"
				color="blue"
				label="Gross Value of Output" />
			<series
				value="val(Firms, nominalGDP, t)"
				color="green"
				label="GDP" />
			<series
				value="sum(Households, consumptionExpenditure, t)
					+sum(Firms, machineInvestment, t) + sum(Firms, operationsInvestment, t)
					-sum(Firms, costsOfGoodsSold, t) - sum(Firms, depreciation, t)"
				color="black"
				label="Net national income" />
			<yAxis label="Monetary units" />
			
		</chart>
			
		<chart title="Price levels" type="time chart">
			<series
				value="agentValue(Markets, MEAN_PRICE, t, B0)"
				color="blue"
				label="B0" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, B1)"
				color="veryLightBlue"
				label="B1" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, B2)"
				color="cyan"
				label="B2" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, B3)"
				color="gray"
				label="B3" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, labor)"
				color="red"
				label="labor" />
			<series
				value="agentValue(Markets, MEAN_PRICE, t, machines)"
				color="green"
				label="machine parts" />
			<yAxis label="Monetary units per good" />
			
		</chart> 
	
		
		<chart title="Firms: Net profit / Revenue" type="time chart">
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=1)):sum(Firms, revenue, t, and(type=1, stage=1))"
				color="blue"
				label="B1" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=2)):sum(Firms, revenue, t, and(type=1, stage=2))"
				color="veryLightBlue"
				label="B2" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=3)):sum(Firms, revenue, t, and(type=1, stage=3))"
				color="cyan"
				label="B3" />
			<series
				value="sum(Firms, netProfit, t, and(type=1, stage=4)):sum(Firms, revenue, t, and(type=1, stage=4))"
				color="gray"
				label="B4" />
			<yAxis label="Percent" />
			
		</chart>  
	
	</col>		
		
	</panel>

XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

	

	<panel title="Households">
	
	<col>
		
		<chart title="Household income"
			   type="time chart">
			<series
				value="agentValue(Markets, turnover, t, labor)"
				color="blue"
				label="Wages" />
			<series
				value="sum(Households, dividendsReceived, t)"
				color="red"
				label="Dividends" />
			<series
				value="sum(Banks, savingsRedemptions, t)+sum(Banks, interestPaidOnSavings, t)"
				color="green"
				label="Redemption" />
			<yAxis label="Monetary units" />
			
		</chart>
		
			
	 	<chart title="Budget Allocation" type="time chart">
			<series
				value="sum(Households, totalBudget, t)"
				color="blue"
				label="Total budget" />
			<series
				value="sum(Households, consumptionExpenditure, t)"
				color="green"
				label="Consumption" />
			<series
				value="sum(Banks, newSavings, t)"
				color="red"
				label="Saving" />
			<series
				value="agentValue(Markets, turnover, t, equity)"
				color="yellow"
				label="Equity purchases" />
			<series
				value="sum(Households, hoarding, t)"
				color="veryLightBlue"
				label="Hoarding" />
			<yAxis label="Monetary units" />
		</chart> 
		
	
		<chart title="Consumption" type="time chart">
			<series
				value="sum(Households, realConsumption, t)"
				color="blue"
				label="Consumer goods bought" />
				<yAxis label="Goods" />
		</chart> 
		
	</col>
		
	<col>
  		<chart title="Asset holdings" type="scatter chart">
			<series
				sector="Households"
				x="timeDeposits"
				y="equityHoldings"
				color="green" 
				label="Households (x=time deposits, y=equity holdings)" />
			<xAxis label="Number of deposits" />
			<yAxis label="Number of shares" />
		</chart> 
		
		<chart title="Income components" type="scatter chart">
			<series
				sector="Households"
				x="dividendsReceived"
				y="wages"
				color="green" 
				label="Households (x=dividends, y=wages)" />
			<xAxis label="Monetary units" />
			<yAxis label="Monetary units" />
		</chart> 
	
		
		<chart title="Income distribution" type="time chart">
			<series
				value="mean(Households, totalBudget, t)"
				color="blue"
				label="Mean total budget" />
			<series
				value="max(Households, totalBudget, t)"
				color="veryLightBlue"
				label="Max" />
			<series
				value="min(Households, totalBudget, t)"
				color="veryLightBlue"
				label="Min" />
			<yAxis label="Monetary units" />
		</chart> 


	</col>
	
	<col>
	
		<chart title="Savings rate" type="time chart">
			<series
				value="(val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t)) : val(Firms, nominalGDP, t)"
				color="veryLightBlue"
				label="Gross savings rate" />
			<series
				value="(val(Firms, nominalGDP, t) - sum(Households, consumptionExpenditure, t) - sum(Firms, depreciation, t)) 
						: (val(Firms, nominalGDP, t) - sum(Firms, depreciation, t))"
				color="orange"
				label="Net savings rate" />
			<series
				value="(agentValue(Markets, turnover, t, equity)+agentValue(Markets, turnover, t, savings)) : sum(Households, totalBudget, t)"
				color="green"
				label="Household savings rate" />
			<yAxis label="Percent" />
		</chart>
		<chart></chart>
		<chart></chart>
	
	</col>		
		
	</panel>

